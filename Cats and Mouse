# hackerrank-solution-in-c-
Algorithm Problem
#include<iostream>
using namespace std;
int main()
{
    int a[1000],x,y,z,n,i,j,b,c,d;
    cin>>n;
  for(i=0;i<n;i++)
  {
      cin>>x>>y>>z;
        b=z-x;c=z-y;
        if(b<0)
        b=-b;
        if(c<0)
        c=-c;
        if(b==c)
        cout<<"Mouse C\n";
        else if(b>c)
        cout<<"Cat B\n";
        else
        cout<<"Cat A\n";
  }
}
