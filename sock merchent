# hackerrank-solution-in-c-
Algorithm Problem
   #include<iostream>
    using namespace std;
    int main()
    {
   int i,j,count,p=0,ar[1000],n;
   cin>>n;
   for(i=0;i<n;i++)
   {
       cin>>ar[i];
   }
    for(i=0;i<n;i++)
    {count=1;
        for(j=i+1;j<n;j++)
        {
            if(ar[i]==ar[j]&&ar[j]!=-10)
          {  count++;
            ar[j]=-10;
          }  
        }
         p=p+count/2;
             
     
    }
cout<<p;}
