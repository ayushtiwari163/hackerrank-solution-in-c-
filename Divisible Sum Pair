# hackerrank-solution-in-c-
Algorithm Problem
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n,k,a[100],i,j,c=0,sum;
    cin>>n>>k;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n;i++)
    {for(j=i+1;j<n;j++)
       { sum=0;
            sum=a[i]+a[j];
         if(sum%k==0)
            c++;
        }
    }
    cout<<c;

}
